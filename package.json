  },
  "homepage": "https://github.com/reindervandervoort/coincap-wrapper#readme",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^7.2.1",
    "eslint": "^7.28.0",

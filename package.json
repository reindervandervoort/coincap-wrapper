    "eslint-plugin-sonarjs": "^0.9.1",
    "nodemon": "^2.0.16",
    "postman-code-generators": "git+https://github.com/postmanlabs/postman-code-generators.git",
    "prettier": "^2.3.1",
    "quicktype": "^15.0.261",
